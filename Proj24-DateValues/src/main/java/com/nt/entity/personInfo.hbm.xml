<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
   <hibernate-mapping>
     <class name="com.nt.entity.PersonInfo" table="PERSONINFO">
       <id name="pid">
          <generator class="sequence"/>
       </id>
          <property name="pname" type="string" length="20"/>
          <property name="paddrs" type="string" length="20"/>
          <property name="dob" type="timestamp"/>
          <property name="dom" type="date"/>
          <property name="doj" type="timestamp"/>
     </class>
   </hibernate-mapping>
